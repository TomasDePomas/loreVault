{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "sql:default",
    "fs:default",
    "sql:default",
    "sql:allow-execute",
    "store:default",
    "dialog:default",
    "fs:default",
    "fs:allow-app-read-recursive",
    "fs:allow-app-read",
    "fs:allow-app-write",
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$APPCACHE/*" }]
    }
  ]
}
